<% for(var i=0; i < data.length; i++) { %>
<div class="container mt-3">
    <div class="row">
        <div class="col-12">
           
            <div class="row text-center">
                <div class="col-12 p-3 newshead">
                    <%=data[i].title%>
                </div>
                <div class="col-12 p-3">
                    <% for(var j=0; j < data[i].text.length; j++) { %>
                        <p class="card-text text-justify"> <%- data[i].icon %> <%= data[i].text[j]%></p>
                        <% } %>
                </div>
            </div>
        </div>

    </div>
</div>
<% } %>
<hr>